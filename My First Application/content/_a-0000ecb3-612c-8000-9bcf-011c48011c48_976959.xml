    <rule>
        <name>MFA_ArrayOperations</name>
        <uuid>_a-0000ecb3-612c-8000-9bcf-011c48011c48_976959</uuid>
        <description>An expression rule that accepts two decimal arrays and calculates the sum of the unique numbers. </description>
        <parentUuid>8c4c3d2a-d85a-494a-8bdc-56449fd36d9b</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>/*Create an expression rule that accepts
two decimal arrays and calculates the 
sum of the unique numbers.*/
sum(
  wherecontains()
  ri!array1+ri!array2
)
</definition>
        <namedTypedValue>
            <name>array1</name>
            <type>
                <name>Decimal?list</name>
                <namespace>http://www.appian.com/ae/types/2009</namespace>
            </type>
        </namedTypedValue>
        <namedTypedValue>
            <name>array2</name>
            <type>
                <name>Decimal?list</name>
                <namespace>http://www.appian.com/ae/types/2009</namespace>
            </type>
        </namedTypedValue>
        <preferredEditor>legacy</preferredEditor>
        <offlineEnabled>false</offlineEnabled>
    </rule>
