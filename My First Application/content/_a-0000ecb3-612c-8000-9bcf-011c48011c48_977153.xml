    <rule>
        <name>MFA_DateFormatting</name>
        <uuid>_a-0000ecb3-612c-8000-9bcf-011c48011c48_977153</uuid>
        <description>An expression rule that takes a date and returns it formatted like "July 24, 2024". </description>
        <parentUuid>8c4c3d2a-d85a-494a-8bdc-56449fd36d9b</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>/*Develop an expression rule that takes a date 
and returns it formatted like "July 24, 2024"*/
datetext(ri!date+1, "MMMM dd, yyyy")</definition>
        <namedTypedValue>
            <name>date</name>
            <type>
                <name>date</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
        </namedTypedValue>
        <preferredEditor>legacy</preferredEditor>
        <offlineEnabled>false</offlineEnabled>
    </rule>
